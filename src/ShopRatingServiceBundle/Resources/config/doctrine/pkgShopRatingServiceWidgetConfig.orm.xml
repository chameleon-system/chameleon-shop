<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\ShopRatingServiceBundle\Entity\pkgShopRatingServiceWidgetConfig" table="pkg_shop_rating_service_widget_config">
        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
            <many-to-one field="CmsTplModuleInstance" target-entity="\ChameleonSystem\DataAccessBundle\Entity\CorePagedef\cmsTplModuleInstance">
    <join-column name="cms_tpl_module_instance_id" referenced-column-name="id" />
    <options>
        <option name="comment">Module instance</option>
    </options>
</many-to-one>
            <many-to-one field="PkgShopRatingService" target-entity="\ChameleonSystem\ShopRatingServiceBundle\Entity\pkgShopRatingService">
    <join-column name="pkg_shop_rating_service_id" referenced-column-name="id" />
    <options>
        <option name="comment">Rating service</option>
    </options>
</many-to-one>
                <lifecycle-callbacks>
                                                                            </lifecycle-callbacks>
        
    </entity>

</doctrine-mapping>