<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\ShopBundle\Entity\ShopOrder\shopOrderVat" table="shop_order_vat">
        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
            <many-to-one field="ShopOrder" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopOrder\shopOrder" inversed-by="ShopOrderVatCollection" >
    <options>
        <option name="comment">Belongs to order</option>
    </options>
        <join-column name="shop_order_id" referenced-column-name="id" />
</many-to-one>
            <field name="Name" type="string" column="name" length="255" nullable="false" >
    <options>
        <option name="comment">Name</option>
        <option name="default"></option>    </options>
</field>
            <field name="VatPercent" type="decimal" column="vat_percent" length="" nullable="false" precision="5" scale="," >
    <options>
        <option name="comment">Percent</option>
        <option name="default"></option>    </options>
</field>
            <field name="Value" type="decimal" column="value" length="" nullable="false" precision="1" scale="0" >
    <options>
        <option name="comment">Value for order</option>
        <option name="default"></option>    </options>
</field>
                <lifecycle-callbacks>
                                                                                                                                    </lifecycle-callbacks>
        
    </entity>

</doctrine-mapping>