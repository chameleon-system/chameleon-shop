<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\ShopBundle\Entity\ShopCore\shopShippingGroup" table="shop_shipping_group">
        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
            <field name="Name" type="string" column="name" length="255" nullable="false" >
    <options>
        <option name="comment">Name</option>
        <option name="default"></option>    </options>
</field>
            <many-to-one field="ShopShippingGroupHandler" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopShippingGroupHandler">
    <join-column name="shop_shipping_group_handler_id" referenced-column-name="id" />
    <options>
        <option name="comment">Shipping group handler</option>
    </options>
</many-to-one>
            <field name="Position" type="integer" column="position" nullable="false" >
    <options>
        <option name="comment">Position</option>
        <option name="default"></option>    </options>
</field>
            <field name="Active" type="boolean" column="active" nullable="false">
    <options>
        <option name="comment">Active</option>
        <option name="default">0</option>    </options>
</field>
            <field name="ActiveFrom" type="datetime" column="active_from" nullable="false" >
    <options>
        <option name="comment">Active from</option>
        <option name="default"></option>    </options>
</field>
            <field name="ActiveTo" type="datetime" column="active_to" nullable="false" >
    <options>
        <option name="comment">Active until</option>
        <option name="default"></option>    </options>
</field>
            <many-to-many field="DataExtranetUserCollection" target-entity="\ChameleonSystem\ExtranetBundle\Entity\dataExtranetUser">
    <join-table name="shop_shipping_group_data_extranet_user_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following customers</option>
    </options>
</many-to-many>
            <many-to-many field="DataExtranetGroupCollection" target-entity="\ChameleonSystem\ExtranetBundle\Entity\dataExtranetGroup">
    <join-table name="shop_shipping_group_data_extranet_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following customer groups</option>
    </options>
</many-to-many>
            <many-to-one field="ShopVat" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopVat">
    <join-column name="shop_vat_id" referenced-column-name="id" />
    <options>
        <option name="comment">VAT group</option>
    </options>
</many-to-one>
            <many-to-many field="ShopShippingTypeCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopShippingType">
    <join-table name="shop_shipping_group_shop_shipping_type_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Shipping types</option>
    </options>
</many-to-many>
            <many-to-many field="ShopPaymentMethodCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\Payment\shopPaymentMethod">
    <join-table name="shop_shipping_group_shop_payment_method_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Payment methods</option>
    </options>
</many-to-many>
            <many-to-many field="ShopShippingGroupCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopShippingGroup">
    <join-table name="shop_shipping_group_shop_shipping_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Is displayed only if the following shipping groups are not available</option>
    </options>
</many-to-many>
            <many-to-many field="CmsPortalCollection" target-entity="\ChameleonSystem\DataAccessBundle\Entity\CorePortal\cmsPortal">
    <join-table name="shop_shipping_group_cms_portal_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to the following portals</option>
    </options>
</many-to-many>
                <lifecycle-callbacks>
                                                                                                                                                                                                                                                                                                                                                                                                </lifecycle-callbacks>
        
    </entity>

</doctrine-mapping>