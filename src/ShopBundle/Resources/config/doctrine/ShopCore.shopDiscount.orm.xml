<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="ChameleonSystem\ShopBundle\Entity\ShopCore\shopDiscount" table="shop_discount">
        <id name="id" type="string" length="36" column="id" unique="true" />
        <field name="cmsident" type="integer" column="cmsident" unique="true">
            <generator strategy="AUTO"/>
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
            <field name="Name" type="string" column="name" length="255" nullable="false" >
    <options>
        <option name="comment">Name</option>
        <option name="default"></option>    </options>
</field>
            <field name="Value" type="decimal" column="value" length="" nullable="false" precision="1" scale="0" >
    <options>
        <option name="comment">Value</option>
        <option name="default"></option>    </options>
</field>
            <field name="ValueType" type="string" column="value_type" length="7" nullable="false" >
    <options>
        <option name="fixed">true</option>
        <option name="comment">Value type</option>
        <option name="default">absolut</option>    </options>
</field>
            <field name="ShowDiscountOnArticleDetailpage" type="boolean" column="show_discount_on_article_detailpage" nullable="false">
    <options>
        <option name="comment">Show percentual discount on detailed product page</option>
        <option name="default">0</option>    </options>
</field>
            <field name="Active" type="boolean" column="active" nullable="false">
    <options>
        <option name="comment">Active</option>
        <option name="default">0</option>    </options>
</field>
            <field name="ActiveFrom" type="datetime" column="active_from" nullable="false" >
    <options>
        <option name="comment">Valid from</option>
        <option name="default"></option>    </options>
</field>
            <field name="ActiveTo" type="datetime" column="active_to" nullable="false" >
    <options>
        <option name="comment">Active until</option>
        <option name="default"></option>    </options>
</field>
            <field name="Position" type="integer" column="position" nullable="false" >
    <options>
        <option name="comment">Sorting</option>
        <option name="default"></option>    </options>
</field>
            <field name="RestrictToArticlesFrom" type="integer" column="restrict_to_articles_from" nullable="false" >
    <options>
        <option name="comment">Min. amount of products affected</option>
        <option name="default"></option>    </options>
</field>
            <field name="RestrictToArticlesTo" type="integer" column="restrict_to_articles_to" nullable="false" >
    <options>
        <option name="comment">Max. amount of products affected</option>
        <option name="default"></option>    </options>
</field>
            <field name="RestrictToValueFrom" type="decimal" column="restrict_to_value_from" length="" nullable="false" precision="1" scale="0" >
    <options>
        <option name="comment">Minimum value of affected products (Euro)</option>
        <option name="default"></option>    </options>
</field>
            <field name="RestrictToValueTo" type="decimal" column="restrict_to_value_to" length="" nullable="false" precision="1" scale="0" >
    <options>
        <option name="comment">Maximum value of affected products (Euro)</option>
        <option name="default"></option>    </options>
</field>
            <many-to-many field="ShopCategoryCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopCategory">
    <join-table name="shop_discount_shop_category_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following product categories</option>
    </options>
</many-to-many>
<field name="ShopCategoryMltInverseEmpty" type="boolean" column="shop_category_mlt_inverse_empty" nullable="false">
    <options>
        <option name="comment">Restrict to following product categories</option>
        <option name="default">0</option>    </options>
</field>
            <many-to-many field="ShopArticleCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\Product\shopArticle">
    <join-table name="shop_discount_shop_article_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following products</option>
    </options>
</many-to-many>
<field name="ShopArticleMltInverseEmpty" type="boolean" column="shop_article_mlt_inverse_empty" nullable="false">
    <options>
        <option name="comment">Restrict to following products</option>
        <option name="default">0</option>    </options>
</field>
            <many-to-many field="DataExtranetGroupCollection" target-entity="\ChameleonSystem\ExtranetBundle\Entity\dataExtranetGroup">
    <join-table name="shop_discount_data_extranet_group_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following customer groups</option>
    </options>
</many-to-many>
<field name="DataExtranetGroupMltInverseEmpty" type="boolean" column="data_extranet_group_mlt_inverse_empty" nullable="false">
    <options>
        <option name="comment">Restrict to following customer groups</option>
        <option name="default">0</option>    </options>
</field>
            <many-to-many field="DataExtranetUserCollection" target-entity="\ChameleonSystem\ExtranetBundle\Entity\dataExtranetUser">
    <join-table name="shop_discount_data_extranet_user_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following customers</option>
    </options>
</many-to-many>
<field name="DataExtranetUserMltInverseEmpty" type="boolean" column="data_extranet_user_mlt_inverse_empty" nullable="false">
    <options>
        <option name="comment">Restrict to following customers</option>
        <option name="default">0</option>    </options>
</field>
            <many-to-many field="DataCountryCollection" target-entity="\ChameleonSystem\DataAccessBundle\Entity\Core\dataCountry">
    <join-table name="shop_discount_data_country_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following shipping countries</option>
    </options>
</many-to-many>
            <field name="Description" type="text" column="description" nullable="false" >
    <options>
        <option name="comment">Description</option>
            </options>
</field>
            <field name="CacheClearLastExecuted" type="datetime" column="cache_clear_last_executed" nullable="false" >
    <options>
        <option name="comment">When has the cache of the affected products been cleared the last time?</option>
        <option name="default"></option>    </options>
</field>
                <lifecycle-callbacks>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        </lifecycle-callbacks>
        
    </entity>

</doctrine-mapping>