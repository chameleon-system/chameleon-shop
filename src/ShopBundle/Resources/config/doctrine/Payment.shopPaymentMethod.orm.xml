    </options>
</many-to-many>
            <many-to-many field="ShopArticle1Collection" target-entity="\ChameleonSystem\ShopBundle\Entity\Product\shopArticle">
    <join-table name="shop_payment_method_shop_article1_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
