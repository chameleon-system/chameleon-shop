    <join-table name="shop_payment_method_shop_category_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following product categories</option>
    </options>
</many-to-many>
            <many-to-many field="ShopArticleCollection" target-entity="\ChameleonSystem\ShopBundle\Entity\Product\shopArticle">
    <join-table name="shop_payment_method_shop_article_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Restrict to following items</option>
    </options>
</many-to-many>
            <many-to-many field="ShopArticleGroup1Collection" target-entity="\ChameleonSystem\ShopBundle\Entity\Product\shopArticleGroup">
    <join-table name="shop_payment_method_shop_article_group1_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
        <option name="comment">Do not allow for following product groups</option>
    </options>
</many-to-many>
            <many-to-many field="ShopCategory1Collection" target-entity="\ChameleonSystem\ShopBundle\Entity\ShopCore\shopCategory">
    <join-table name="shop_payment_method_shop_category1_mlt">
        <join-columns>
            <join-column name="source_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
            <join-column name="target_id" referenced-column-name="id"/>
        </inverse-join-columns>
    </join-table>
    <options>
